{
  "compileOnSave": false,
  "compilerOptions": {
    "skipLibCheck": true,
    "preserveConstEnums": true,
    "strict": true,
    "inlineSources": true,
    "inlineSourceMap": true,
    "sourceRoot": "./src",
    "target": "esnext",
    "outDir": "dist/",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "baseUrl": "./src",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "types": ["jest", "node"],
    "strictPropertyInitialization": false
  },
  "noEmit": true,
  "include": [
    "src/**/*.ts",
    "backups/push.ts",
    "backups/receipt_test_android.ts",
    "backups/receipt_test_ios.ts"
  ],
  "exclude": ["node_modules"]
}
