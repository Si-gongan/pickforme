{
    "cli": {
        "version": ">= 3.13.3"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "android": {
                "buildType": "apk",
                "gradleCommand": ":app:assembleDebug",
                "env": {
                    "ANDROID_HOME": "/Users/shareemotion/Library/Android/sdk"
                }
            }
        },
        "preview": {
            "distribution": "internal"
        },
        "production": {
            "channel": "production",
            "android": {
                "credentialsSource": "local"
            },
            "ios": {
                "credentialsSource": "remote"
            }
        },
        "develop": {
            "distribution": "store"
        }
    },
    "submit": {
        "production": {},
        "development": {}
    }
}
