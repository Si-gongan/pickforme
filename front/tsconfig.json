{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@stores": ["./stores/index.ts"],
      "@types": ["./types/index.ts"],
      "@services": ["./services/index.ts"],
      "@components": ["./components/index.ts"],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
