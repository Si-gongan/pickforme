{
    "expo": {
        "owner": "sigongan",
        "name": "픽포미",
        "slug": "pickformeapp",
        "version": "3.2.0",
        "orientation": "portrait",
        "icon": "./assets/images/pickforme_logoWtxt_200x200.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "extra": {
            "eas": {
                "projectId": "fc30c083-8843-43b4-9abc-5754cd747988"
            },
            "router": {
                "origin": false
            }
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.sigonggan.pickforme",
            "jsEngine": "jsc",
            "icon": "./assets/images/pickforme_logo_200x200.png",
            "googleServicesFile": "./GoogleService-Info.plist"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/pickforme_logo_200x200.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.anonymous.pickforme",
            "googleServicesFile": "./google-services.json"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/pickforme_logoWtxt_200x200.png"
        },
        "plugins": [
            ["@react-native-firebase/app", "@react-native-firebase/analytics"],
            [
                "@react-native-seoul/kakao-login",
                {
                    "kakaoAppKey": "a48d1b07defd794b769579746e2bb995",
                    "kotlinVersion": "1.9.0"
                }
            ],
            "expo-router",
            "expo-apple-authentication",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/pickforme_logoWtxt_200x200.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ],
            [
                "expo-build-properties",
                {
                    "android": {
                        "extraMavenRepos": ["https://devrepo.kakao.com/nexus/content/groups/public/"]
                    },
                    "ios": {
                        "useFrameworks": "static"
                    }
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "runtimeVersion": "1.0.0",
        "updates": {
            "url": "https://u.expo.dev/fc30c083-8843-43b4-9abc-5754cd747988"
        }
    }
}
