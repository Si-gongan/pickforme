{
    "expo": {
        "owner": "sigongan",
        "name": "픽포미",
        "slug": "pickforme",
        "version": "3.1.2",
        "orientation": "portrait",
        "icon": "./assets/images/pickforme_logoWtxt_200x200.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.sigonggan.pickforme",
            "jsEngine": "jsc",
            "icon": "./assets/images/pickforme_logo_200x200.png"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/pickforme_logo_200x200.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.anonymous.pickforme"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/pickforme_logoWtxt_200x200.png"
        },
        "plugins": [
            [
                "@react-native-seoul/kakao-login",
                {
                    "kakaoAppKey": "a48d1b07defd794b769579746e2bb995",
                    "kotlinVersion": "1.9.0"
                }
            ],
            "expo-router",
            "expo-apple-authentication",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/pickforme_logoWtxt_200x200.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ],
            [
                "expo-build-properties",
                {
                    "android": {
                        "extraMavenRepos": ["https://devrepo.kakao.com/nexus/content/groups/public/"]
                    }
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}
